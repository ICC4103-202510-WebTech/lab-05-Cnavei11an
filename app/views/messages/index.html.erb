<h1>Messages</h1>
<table>
  <tr>
    <th>ID</th><th>Chat ID</th><th>User ID</th><th>Body</th><th>Actions</th>
  </tr>
  <% @messages.each do |message| %>
    <tr>
      <td><%= message.id %></td>
      <td><%= message.chat_id %></td>
      <td><%= message.user_id %></td>
      <td><%= message.body %></td>
      <td><%= link_to "View", message_path(message.id) %></td>
    </tr>
  <% end %>
</table>
